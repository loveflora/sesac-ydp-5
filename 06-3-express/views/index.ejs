<!-- ejs도 html -->
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>메인 페이지</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
      crossorigin="anonymous"
    ></script>
  </head>
  <body>
    <ul
      class="nav nav-pills nav-fill gap-2 p-1 small bg-primary rounded-5 shadow-sm"
      id="pillNav2"
      role="tablist"
      style="
        --bs-nav-link-color: var(--bs-white);
        --bs-nav-pills-link-active-color: var(--bs-primary);
        --bs-nav-pills-link-active-bg: var(--bs-white);
      "
    >
      <li class="nav-item" role="presentation">
        <button
          class="nav-link active rounded-5"
          id="home-tab2"
          data-bs-toggle="tab"
          type="button"
          role="tab"
          aria-selected="true"
        >
          Home
        </button>
      </li>
      <li class="nav-item" role="presentation">
        <a href="/login"><button
          class="nav-link rounded-5"
          id="profile-tab2"
          data-bs-toggle="tab"
          type="button"
          role="tab"
          aria-selected="false"
        >
        <div style="color: white">Login</div>
        </button></a>
      </li>
      <li class="nav-item" role="presentation">
        <a href="/register"> <button
          class="nav-link rounded-5"
          id="contact-tab2"
          data-bs-toggle="tab"
          type="button"
          role="tab"
          aria-selected="false"
        >
         <div style="color: white">Sign Up</div></a>
        </button>
      </li>
    </ul>
    <%- include('./includes/header') %>
    <!-- 다른 view파일 불러올 때 "<%#- include('view의 상대주소') %>" -->
    <!-- 주석 안먹어서 따로 # 붙여야 함 -->
    <h1>HELLO ! EXPRESS</h1>
    <p>안녕하세요 <%= userId%>님 ! 환영합니다.</p>
    <p>당신의 PW는 <%= userPw%>입니다.</p>
    
    <!-- 자바스크립트 코드 : 반복문 -->
    <!-- 자바스크립트 코드 매 줄마다 "<% %>" -->
    <% for(let i = 0; i < 5; i++) { %>
      <div><%= i %></div>
      <!-- 값을 템플릿에 출력할 때 "<%= %>" -->
    <% } %>
  </body>ㄴ
</html>
